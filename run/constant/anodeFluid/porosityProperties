{
    type            myDarcyForchheimer;
    active          true;
    cellZone        agdl;

    porosity        0.5;

    Cp              Cp [0 2 -2 -1 0 0 0]    710.0;
    k               k  [1 1 -3 -1 0 0 0]      1.6;
    
