FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/anodeFluid";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default    Gauss linear;
    grad(p)    Gauss linear;
}

divSchemes
{
    default                       none;
    div(phifuel,U)                Gauss GammaV 0.2;
    div(phifuel,y)                Gauss upwind;
    div(phifuel,S)                Gauss upwind;
}

laplacianSchemes
{
    default                    none;
    laplacian(mu,U)            Gauss harmonic corrected;
    laplacian((rho|A(U)),p)    Gauss linear corrected;
    laplacian(gamma,y)         Gauss harmonic corrected;
    laplacian(diff,y)          Gauss harmonic corrected;
    laplacian(diff,S)          Gauss linear corrected; // changed from harmonic
}

interpolationSchemes
{
    default            linear;
    interpolate(rho)   harmonic;
    interpolate(T)     harmonic;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p;
}


// ************************************************************************* //
