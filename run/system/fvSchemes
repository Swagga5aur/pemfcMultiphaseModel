FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default              none;
    div(rhoCpPhi,T)      Gauss upwind;
}

laplacianSchemes
{
    default              none;
    laplacian(k,T)       Gauss harmonic corrected;
}

interpolationSchemes
{
    //default         harmonic;
    default         linear;
    interpolate(T)  harmonic;
    interpolate(k)  harmonic;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    T;
}


